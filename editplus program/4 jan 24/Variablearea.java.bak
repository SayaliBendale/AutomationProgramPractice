class variablearea 
{
	public static void main(String[] args) 
	{
		System.out.println("Program Start");
		float area;
		float r;
		float pi;
		r=4;
		pi=3.14;
		area=pi*r*r;
		System.out.println("r="+r);
		System.out.println("pi="+pi);
		System.out.println("area="+area);
		System.out.println("Program End ");
	}
}
